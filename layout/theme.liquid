     <style>
 html {
  scroll-behavior: smooth;
}
    
    .da_body_home {
      {% if settings.c_home-bg != blank %}
      background-image: url('{{settings.c_home-bg | img_url : "master"}}');
      {% endif %}
      background-repeat: no-repeat;
      background-size: cover;
      background-position: center center;
      background-attachment: fixed;
/*       backdrop-filter: blur(104px); */
      padding: 70px 50px;
    }

    .da_body_main {
      background-color: {{settings.c_home-color}};
      width: 100%;
      max-width: 750px;
      margin: 0px auto;
      border-radius: 12px;
    }
       {% if template == 'page.c-home' %}
html body.announcement-hidden .main-content[data-page-type=page] {
    padding-top: 0px;
}
         {% endif %}

    @media (max-width: 768px){
      .da_body_home {
        padding: 0px;
      }
      
      .da_body_main {
        max-width: 100%;
        border-radius: 0px;
      }
    }
    
  </style>
</head>
<body
    id="{{ page_title | handle }}"
    class="
        {{ page_title | handle }}
        {% if customer %}customer-logged-in {% endif %} 
        template-{% if request.page_type != blank %}{{ request.page_type | handle }}{% else %}none{% endif %}
        data-animations
        announcement-hidden {% if template == 'page.c-home' %}da_body_home{% endif %}">
  {% render 'elevar-body-end' %}

    {% include '23_02__body-tracking' %}

  {% unless template == 'page.c-home' %}
    {% section 'header' %}
    {% endunless %}

    <div class="main-content{% if request.page_type == blank %} main-content--no-template{% endif %}" data-page-type="{{ request.page_type }}" data-drawer-push>
        <div class="main-content__wrapper">
            <main class="content-for-layout {% if template == 'page.c-home' %}da_body_main{% endif %}" id="MainContent" role="main">
                {{ content_for_layout }}
            </main>

            {% unless template == 'page.c-home' %}
            {% section 'footer' %}
              {% endunless %}
        </div>
    </div>



    {% include '23_02__cart-drawer' %}


    {% include '23_02__footer-scripts' %}




    {% section '23_02__comment-out' %}





    {% comment %}
    <script>window.KlarnaThemeGlobals={};{%case template.name%} {%when"product"%} {% if product.first_available_variant.price %}window.KlarnaThemeGlobals.data_purchase_amount = {{ product.first_available_variant.price }};{% endif %}window.KlarnaThemeGlobals.productVariants={{product.variants|json}};window.KlarnaThemeGlobals.documentCopy=document.cloneNode(true);{%when"cart"%} window.KlarnaThemeGlobals.data_purchase_amount={{cart.total_price}};window.KlarnaThemeGlobals.documentCopy=document.cloneNode(true);{%endcase%}</script>

