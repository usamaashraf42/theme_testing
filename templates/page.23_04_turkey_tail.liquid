// ---------------------------------------------
// Count down timer
// ---------------------------------------------

// Set the date we're counting down to
  var countDownDate = new Date("Apr 16, 2023 23:59:59").getTime();

// Get today's date and time
  var now = new Date().getTime();

  if (countDownDate > now) { // Update the count down every 1 second
    var x = setInterval(function() {

// Get today's date and time
      var now = new Date().getTime();

// Find the distance between now and the count down date
      var distance = countDownDate - now;

// Time calculations for days, hours, minutes and seconds
      var days = Math.floor(distance / (1000 * 60 * 60 * 24));
      var day_array = String(days)
        .split("")
        if (day_array.length == 1) {
          var day_1 = "0"
          var day_2 = String(day_array[0])
        } else {
          var day_1 = String(day_array[0])
          var day_2 = String(day_array[1])
        }
        document
        .querySelectorAll(".time-day-1-digit")
        .forEach(function(elem) {
          elem.innerHTML = day_1
        })
        document
        .querySelectorAll(".time-day-2-digit")
        .forEach(function(elem) {
          elem.innerHTML = day_2
        })


      var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
      var hours_array = String(hours)
        .split("")
        if (hours_array.length == 1) {
          var hour_1 = "0"
          var hour_2 = String(hours_array[0])
        } else {
          var hour_1 = String(hours_array[0])
          var hour_2 = String(hours_array[1])
        }
        document
        .querySelectorAll(".time-hour-1-digit")
        .forEach(function(elem) {
          elem.innerHTML = hour_1
        })
        document
        .querySelectorAll(".time-hour-2-digit")
        .forEach(function(elem) {
          elem.innerHTML = hour_2
        })


      var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
      var minutes_array = String(minutes)
        .split("")
        if (minutes_array.length == 1) {
          var minute_1 = "0"
          var minute_2 = String(minutes_array[0])
        } else {
          var minute_1 = String(minutes_array[0])
          var minute_2 = String(minutes_array[1])
        }
        document
        .querySelectorAll(".time-minute-1-digit")
        .forEach(function(elem) {
          elem.innerHTML = minute_1
        })
        document
        .querySelectorAll(".time-minute-2-digit")
        .forEach(function(elem) {
          elem.innerHTML = minute_2
        })


      var seconds = Math.floor((distance % (1000 * 60)) / 1000);
      var seconds_array = String(seconds)
        .split("")
        if (seconds_array.length == 1) {
          var second_1 = "0"
          var second_2 = String(seconds_array[0])
        } else {
          var second_1 = String(seconds_array[0])
          var second_2 = String(seconds_array[1])
        }
        document
        .querySelectorAll(".time-second-1-digit")
        .forEach(function(elem) {
          elem.innerHTML = second_1
        })
        document
        .querySelectorAll(".time-second-2-digit")
        .forEach(function(elem) {
          elem.innerHTML = second_2
        })

    }, 1000);
  }
