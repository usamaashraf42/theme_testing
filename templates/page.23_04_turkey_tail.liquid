  var toggleBtns = document.querySelectorAll(".faq-toggle-btn")
  toggleBtns.forEach(function(elem) {
    elem.addEventListener("click", function(e) {
      e.preventDefault();
      if (elem.classList.contains("faq-closed")) {
        elem
          .classList
          .remove("faq-closed")
          elem
          .classList
          .add("faq-open")
        elem
        .closest(".faq-item")
        .classList
        .remove("faq-hidden")
      } else {
        elem
          .classList
          .add("faq-closed")
          elem
          .classList
          .remove("faq-open")
        elem
        .closest(".faq-item")
        .classList
        .add("faq-hidden")
      }
    });
  })


// ---------------------------------------------
// Subscription selector
// ---------------------------------------------

var subscriptionOptions = document.querySelectorAll(".subscription-option");
subscriptionOptions.forEach(function(elem) {
  elem.addEventListener("click", function(e) {
    e.preventDefault();

    var subscriptionVariantId = elem.dataset.subscriptionOptionVariantId
    var sellingPlanId = elem.dataset.productSellingPlanId
  
    // Change green highlighting
    subscriptionOptions.forEach(function(elem) {
        elem.classList.remove("selected-subscription")
    })
    elem.classList.add("selected-subscription")

    // Change how dirtea will be shipped
    document.querySelectorAll(".subscription-sumary").forEach(function(elem){
      elem.classList.add("subsciption-summary-hidden")
    })
    document.querySelector(`.subscription-sumary[data-subscription-summary-variant-id="${subscriptionVariantId}"]`).classList.remove("subsciption-summary-hidden")

    // Change which product the button adds to cart
    var subscribeBtn = document.querySelector(".subscribe-btn-add-to-cart")
    subscribeBtn.dataset.productVariantId = subscriptionVariantId
    subscribeBtn.dataset.productSellingPlanId = sellingPlanId

    })
})
</script>