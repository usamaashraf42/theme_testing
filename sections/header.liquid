{% comment %}
{% if section.settings.enable_transparent and request.page_type == 'index' %}
  <style>
    html:not(.supports-no-js) body:not(.navigation-open) .site-header--transparent:not(.site-header--fixed) .burger-icon {
      background-color: {{ section.settings.transparent_text_color }};
    }
    html:not(.supports-no-js) body:not(.navigation-open) .site-header--transparent:not(.site-header--fixed) .site-header__cart .icon,
    html:not(.supports-no-js) body:not(.navigation-open) .site-header--transparent:not(.site-header--fixed) .site-header__bag .icon {
      fill: {{ section.settings.transparent_text_color }};
    }
    html:not(.supports-no-js) body:not(.navigation-open) .site-header--transparent:not(.site-header--fixed) a.site-header__logo {
      color: {{ section.settings.transparent_text_color }};
    }
    html:not(.supports-no-js) body:not(.navigation-open) .site-header--transparent:not(.site-header--fixed) .site-header__navigation::after,
    html:not(.supports-no-js) body:not(.navigation-open) .site-header--transparent:not(.site-header--fixed) .site-header__cart::after {
      border-color: {{ section.settings.transparent_text_color }};
    }
  </style>
{% endif %}
{% endcomment %}


<header class="dirtea-header dirtea-header-transparent">
    <div class="dirtea-loading-container hide"><div class="dirtea-loading-indicator"></div></div>
    <div class="dirtea-hero-scrim"></div>

    <div class="announcement-container hide">
        <div class="announcement">
        {% for block in section.blocks %}
        {% if block.settings.Announcement_url %}
            <a href="{{ block.settings.Announcement_url }}" class="one-whole">
                <span>{{ block.settings.Announcement_text }}</span>
            </a>
        {% else %}
            <span>{{ block.settings.Announcement_text }}</span>
        {% endif %}
        {% endfor %}
        </div>
        
        <button class="btn btn--clear">
            <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-close" viewBox="0 0 16.8 16.8"><path fill="#444" d="M16.8 1.5L15.4.1 8.4 7l-7-7L0 1.4l7 7-7 7 1.4 1.4 7-7 7 7 1.4-1.4-7-7z"></path></svg>
        </button>
    </div>

    <div class="navigation-container mobile-menu-closed">

        <div class="nav-half-section nav-left dirtea-header-hide-mobile">
            <ul>
            {% for link in linklists.nav-primary-left.links %}
                <li><a href="{{ link.url }}">{{ link.title }}</a></li>
            {% endfor %}
            </ul>
        </div>

        <div class="nav-half-section nav-left nav-hamburger-menu dirtea-header-hide-desktop">
            <div class="dirtea-menu-toggle-display dirtea-menu-closed-button">
                <button class="btn btn--clear btn--square">
                <span class="burger-icon burger-icon--top"></span>
                <span class="burger-icon burger-icon--mid"></span>
                <span class="burger-icon burger-icon--bottom"></span>
                <span class="icon__fallback-text">Menu</span>
                </button>
            </div>
            <div class="dirtea-menu-toggle-display dirtea-menu-open-button hide">
                <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-close" viewBox="0 0 16.8 16.8"><path fill="#444" d="M16.8 1.5L15.4.1 8.4 7l-7-7L0 1.4l7 7-7 7 1.4 1.4 7-7 7 7 1.4-1.4-7-7z"></path></svg>
            </div>
        </div>

        <div class="dirtea-mobile-menu-items" style="display:none;" data-menu-state="hidden">
            <ul>
            {% for link in linklists.nav-primary-left.links %}
                <li><a href="{{ link.url }}">{{ link.title }}</a></li>
            {% endfor %}
            {% for link in linklists.nav-primary-right.links %}
                <li><a href="{{ link.url }}">{{ link.title }}</a></li>
            {% endfor %}
            {% if customer %}
                <li><a href="/account/logout">Logout</a>
            {% else %}
                <li><a href="{{ routes.account_login_url }}">Login</a>
            {% endif %}
            </ul>
            <div class="dirtea-mobile-menu-mushroom">
                <img src="{{ '23_02__dirtea-mushroom.svg' | asset_url }}">
            </div>

        </div>

        <div class="brand-center {{ template.name  }}">
          <a href="/">
            <div class="dirtea-logo-block-container"></div>
              {% comment  %}
              <img src="{{ '23_02__dirtea-logo-white.svg' | asset_url }}">
              {% endcomment %}
          </a>
            {% comment %}
            <img class="dirtea-logo" src="{{ section.settings.logo | img_url: image_size }}" srcset="{{ section.settings.logo | img_url: image_size }} 1x, {{ section.settings.logo | img_url: image_size, scale: 2 }} 2x" alt="{{ section.settings.logo.alt | default: shop.name }}" itemprop="logo">
            {% endcomment %}
        </div>

        <div class="nav-half-section nav-right dirtea-header-hide-mobile">
            <ul>
            {% for link in linklists.nav-primary-right.links %}
                <li><a href="{{ link.url }}">{{ link.title }}</a></li>
            {% endfor %}
            {% if customer %}
                <li><a href="/account">Account</a>
                <li><a href="/account/logout">Logout</a>
            {% else %}
                <li><a href="{{ routes.account_login_url }}">Login</a>
            {% endif %}
            </ul>
        </div>

        <div class="nav-half-section nav-right dirtea-cart-btn-container">

            {% if 4==5 %}
            <a href="{{ routes.cart_url }}" class="btn btn--clear btn--square btn--hover-scale">
                <img src="{{ '23_02__dirtea-mushroom.svg' | asset_url }}">
                <div class="cart-counter-container {% if cart.item_count < 0 %}hide{% endif %}">
                    <span class="cart-count">{{ cart.item_count }}</span>
                </div>
            </a>
            {% else %}
            <a href="#" class="btn btn--clear btn--square btn--hover-scale">
                <img src="{{ '23_02__dirtea-mushroom.svg' | asset_url }}">
                <div class="cart-counter-container {% if cart.item_count < 0 %}hide{% endif %}">
                    <span class="cart-count">{{ cart.item_count }}</span>
                </div>
            </a>
            {% endif %}

        </div>

    </div>
</header>


