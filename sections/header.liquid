<script>
// Remove announcement bar on close and store state in local storage
announcementBlock = document.querySelector(".announcement-container");
announcement_close_button = document.querySelector(".announcement-container button");
announcement_close_button.addEventListener("click", function(){
    announcementBlock.classList.add("hide");
    document.querySelector("body").classList.add("announcement-hidden");
    localStorage.setItem('dirtea-announcement-state', 'dismissed');
});
userAnnouncementState = localStorage.getItem('dirtea-announcement-state');
if( userAnnouncementState == "dismissed" ){
    announcementBlock.classList.add("hide");
    document.querySelector("body").classList.add("announcement-hidden");
} else {
    announcementBlock.classList.remove("hide");
    document.querySelector("body").classList.remove("announcement-hidden");
}


// Add class to .dirtea-header on scroll-position larger than x
    dirteaHeader = document.querySelector(".dirtea-header");
document.addEventListener('scroll', function(){
    if (window.scrollY > 30) {
        dirteaHeader.classList.remove("dirtea-header-transparent");
        dirteaHeader.classList.add("bg-light-grey");
        dirteaHeader.classList.add("nav-box-shadow");
    } else {
        dirteaHeader.classList.add("dirtea-header-transparent");
        dirteaHeader.classList.remove("bg-light-grey");
        dirteaHeader.classList.remove("nav-box-shadow");
    }
}, { passive: true });


// Show / hide menu on mobile
menuButton = document.querySelectorAll(".dirtea-header .dirtea-menu-toggle-display")
menuOverlay = document.querySelector(".dirtea-header .dirtea-mobile-menu-items")
dirteaNavigationContainer = document.querySelector(".dirtea-header .navigation-container")

menuButton.forEach(function(elem) {
    elem.addEventListener("click", function(e) {
        if (menuOverlay.dataset.menuState == "hidden"){
            menuOverlay.style.display = "block"
            menuOverlay.dataset.menuState = "visible"
            dirteaNavigationContainer.classList.remove("mobile-menu-closed")
            dirteaNavigationContainer.classList.add("mobile-menu-open")
            document.querySelector(".dirtea-menu-closed-button").classList.add("hide")
            document.querySelector(".dirtea-menu-open-button").classList.remove("hide")
        } else {
            menuOverlay.style.display = "none"
            menuOverlay.dataset.menuState = "hidden"
            dirteaNavigationContainer.classList.add("mobile-menu-closed")
            dirteaNavigationContainer.classList.remove("mobile-menu-open")
            document.querySelector(".dirtea-menu-closed-button").classList.remove("hide")
            document.querySelector(".dirtea-menu-open-button").classList.add("hide")
        }
    })
});

</script>



